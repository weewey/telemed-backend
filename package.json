{
  "name": "qdoc",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:setup": "sequelize db:drop; sequelize db:create && sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:delete-seed": "sequelize db:seed:undo:all",
    "test": "npm run lint && TZ='Asia/Singapore' ./node_modules/.bin/jest --maxWorkers=1 --detectOpenHandles --coverage --forceExit ",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx 'src/' '__tests__/'",
    "lint:fix": "npm run lint -- --fix",
    "test:watch": "TZ='Asia/Singapore' jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hau-zy/qdoc.git"
  },
  "dependencies": {
    "@types/express-validator": "^3.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-winston": "^4.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest-when": "^2.7.2",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/rosie": "^0.0.38",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-html-reporter": "^3.3.0",
    "jest-when": "^3.2.1",
    "rosie": "^2.1.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6"
  }
}
